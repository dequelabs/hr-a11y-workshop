{
  "name": "hr-a11y",
  "private": true,
  "version": "0.1.0",
  "description": "An application for demoing the full Deque Worldspace tool suite.",
  "main": "./server.js",
  "scripts": {
    "setup": "cd admin && npm run setup",
    "start": "node server.js",
    "strapi": "node_modules/strapi/bin/strapi.js",
    "lint": "node_modules/.bin/eslint api/**/*.js config/**/*.js plugins/**/*.js",
    "postinstall": "node node_modules/strapi/lib/utils/post-install.js",
    "build": "webpack",
    "start:dev": "webpack-dev-server",
    "test": "rm -rf a11y-results; jasmine --config=jasmine.json; attest-report ./a11y-results -d ./a11y-reports --format html+junit",
    "aget": "rm -rf aget/testOutput && aget spec ./aget/specs/main.json ./aget/testOutput --format html+junit",
    "dashboard": "cd ./dashboard && serve"
  },
  "devDependencies": {
