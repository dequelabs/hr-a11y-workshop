{
    "projects": [{
        "name": "localhost:3001",
        "id": "cli_demo",
        "pageList": [{
            "name": "Login",
            "url": "http://localhost:3001/login",
            "actions": [
                "analyze with title \"Deque Systems\"",
                "type \"tester\" into element \"#username\"",
                "type \"123@123\" into element \"#password\"",
                "click element \"form > button\""
            ]
        },{
            "name": "Home",
            "url": "http://localhost:3001",
            "actions": [
                "analyze with title \"HR A11y\"",
                "click element \"#openPTOModal\"",
                "type \"8\" into element \"#PTORequestModal input#hours\"",
                "type \"This is a test PTO request.\" into element \"#PTORequestModal textarea#description\"",
                "dismiss modal \"#PTORequestModal\" with close button \"#PTORequestModal #submitPTORequest\""
            ]
        },{
            "name": "Timesheets",
            "url": "http://localhost:3001/timesheets",
            "actions": [
                "analyze with title \"Timesheets\"",
                "click element \"#action-buttons > button\"",
                "analyze",
                "type \"My Project\" into element \"#project\"",
                "type \"2\" into element \"#monday\"",
                "type \"4\" into element \"#tuesday\"",
                "type \"6\" into element \"#wednesday\"",
                "type \"8\" into element \"#thursday\"",
                "type \"10\" into element \"#friday\"",
                "click element \"#saveTimesheet\""
            ]
        }]
    }]
}